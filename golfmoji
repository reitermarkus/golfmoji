#!/usr/bin/env ruby

require_relative 'lib/golfmoji'

module Golfmoji
  ['â›³', 'ğŸ’¥', 'Â©', "\u{1F397}"].each do |f|
    exec(f)
  end
  p 'result:'
  p peek

  ['â›³', 'ğŸ’¥', "'", 'ğŸ“¦', 'ğŸ™Š', 'ğŸ”—'].each do |f|
    exec(f)
  end
  p 'result:'
  p peek

  ['â›³', ' ', 'âœ‚', '-', 'ğŸ”—'].each do |f|
    exec(f)
  end
  p 'result:'
  p peek

  ['â›³', ' ', 'âœ‚', 'â›³', 'âœ'].each do |f|
    exec(f)
  end
  p 'result:'
  p peek

  # https://codegolf.stackexchange.com/questions/126410/sugar-free-syntax
  reset(['abc']) # = input
  ['ğŸ’¥', "'", 'ğŸ“¦', '[]', "\u{1F587}", ':', 'ğŸ”—'].each do |f|
    exec(f)
  end
  p 'result:'
  p peek

  # https://codegolf.stackexchange.com/questions/126373/am-i-a-fibonacci-number
  reset(['4']) # = input
  ['ğŸ”'].each do |f|
    exec(f)
  end
  p 'result:'
  p peek

  # https://codegolf.stackexchange.com/questions/126159/group-a-list-by-frequency
  reset(['[1,1,1,4,5,6,6,6,7,7,8,8,8,8,8,8,8,9,5,6,5,6,5,6,5,6,-56]'])
  ['ğŸ“ƒ', 'ğŸš¬'].each do |f|
    exec(f)
  end
  p 'result:'
  p peek
end
